import java.util.Scanner;

public class maps {

    public static void main(String[] args) {

        System.out.println("Write the name of your country:");
        Scanner country = new Scanner(System.in);
        String c = country.nextLine();

        System.out.println("Click in the link, for the map of your country: https://www.google.com/maps/search/" + c );
    }
}
